<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="200.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.MainMenuController">
   <children>
      <ChoiceBox fx:id="choiceBox" layoutX="75.0" layoutY="100.0" prefWidth="150.0" />
      <Button fx:id="Start" layoutX="93.0" layoutY="132.0" mnemonicParsing="false" onAction="#StartButton" text="Start" />
      <TextField fx:id="GridSizey" layoutX="76.0" layoutY="68.0" promptText="Grid Y" />
      <TextField fx:id="GridSizex" layoutX="76.0" layoutY="37.0" promptText="Grid X" />
      <Button fx:id="Clear" layoutX="161.0" layoutY="132.0" mnemonicParsing="false" onAction="#clearButton" text="Clear" />
   </children>
</AnchorPane>
